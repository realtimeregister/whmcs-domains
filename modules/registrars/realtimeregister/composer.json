{
    "name": "registrars/realtimeregister",
    "type": "library",
    "version": "2.0.0",
    "autoload": {
        "psr-4": {
            "RealtimeRegisterDomains\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Realtime Register",
            "email": "info@realtimeregister.com",
            "homepage": "https://realtimeregister.com"
        }
    ],
    "support": {
        "email": "support@realtimeregister.com",
        "docs": "https://kb.realtimeregister.com/category/100-whmcs",
        "security": "https://realtimeregister.com/.well-known/security.txt",
        "source": "https://github.com/realtimeregister/whmcs"

    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.1",
        "realtimeregister/realtimeregister-php": "^v1.1.7",
        "giggsey/libphonenumber-for-php-lite": "^8.13",
        "psr/log": "^1.0.0",
        "illuminate/cache": "^7.12"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.4",
        "phpunit/phpunit": "^10.5",
        "squizlabs/php_codesniffer": "^3.10",
        "phpcompatibility/php-compatibility": "^9.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "illuminate/database": "^7.12",
        "illuminate/events": "^7.12",
        "smarty/smarty": "^3.1",
        "true/punycode": "^2.1",
        "guzzlehttp/guzzle": "^7.2|^7.9.3"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
